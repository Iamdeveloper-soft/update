{
  "manifest_version": 3,
  "name": "Data Separator",
  "version": "1.0",
  "description": "Processes rows of numbers and categorizes them into wireless, VOIP, and landlines.",
  "permissions": [
    "storage",
    "identity",
    "identity.email",
    "alarms",
    "notifications",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://yourdomain.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
